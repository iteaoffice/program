{% do form.prepare() %}
{{ form().openTag(form)|raw }}

{% set program = form.get('program_entity_program') %}

{{ ztbformelement(program.get('id')) }}
{{ ztbformelement(program.get('program')) }}
{{ ztbformelement(program.get('number')) }}
{{ ztbformelement(program.get('cluster')) }}
{{ ztbformelement(program.get('invoiceMethod')) }}
{{ formelement(form.get('csrf')) }}
<div class="form-group row">
    <div class="offset-sm-3 col-sm-9">
        {{ formelement(form.get('submit')) }}
        {% if form.has('delete') %}
            {{ formelement(form.get('delete')) }}
        {% endif %}
        {{ formelement(form.get('cancel')) }}
    </div>
</div>

{{ form().closeTag()|raw }}
