{
  "name": "iteaoffice/program",
  "type": "library",
  "license": "proprietary",
  "description": "Code to manage program",
  "keywords": [
    "program"
  ],
  "homepage": "https://itea3.org",
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.itea3.eu"
    }
  ],
  "authors": [
    {
      "name": "Johan van der Heide",
      "email": "johan.van.der.heide@itea3.org"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php": "^7.2",
    "zendframework/zend-mvc":"^3.0.1",
    "phpoffice/phpword": "0.14.*"
  },
  "require-dev": {
    "iteaoffice/testing": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "Program\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ProgramTest\\": "test/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "3.0.x-dev",
      "dev-develop": "3.1.x-dev"
    }
  }
}
