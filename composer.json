{
  "name": "iteaoffice/program",
  "type": "library",
  "license": "proprietary",
  "description": "Code to manage program",
  "keywords": [
    "program"
  ],
  "homepage": "https://itea3.org",
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.itea4.org"
    }
  ],
  "authors": [
    {
      "name": "Johan van der Heide",
      "email": "johan.van.der.heide@itea3.org"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "zendframework/zend-mvc": "^3.0.1",
    "zendframework/zend-mvc-form": "^1.0.0",
    "zendframework/zend-code": "3.3.1",
    "zendframework/zend-mvc-console": "^1.2.0",
    "zendframework/zend-mvc-plugins": "^1.0.1",
    "zendframework/zend-navigation": "^2.9.0",
    "jield-webdev/bjy-authorize": "^1.4.0",
    "doctrine/doctrine-orm-module": "@stable",
    "phpoffice/phpword": "^0.16"
  },
  "require-dev": {
    "iteaoffice/testing": "^1.2",
    "iteaoffice/project": "^4.1",
    "iteaoffice/contact": "^4.0",
    "iteaoffice/search": "^1.1"
  },
  "autoload": {
    "psr-4": {
      "Program\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ProgramTest\\": "test/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "4.0.x-dev",
      "dev-develop": "4.1.x-dev"
    }
  },
  "scripts": {
    "check": [
      "@cs",
      "@test"
    ],
    "fix": [
      "@cbf",
      "@cs-fix"
    ],
    "lint": [
      "@php-lint"
    ],
    "php-lint": "parallel-lint module",
    "cs": "phpcs -n -p --standard=PSR2 --extensions=php,twig module",
    "cbf": "phpcbf module --standard=PSR2",
    "cs-fix": "php-cs-fixer fix -v module",
    "test": "phpunit"
  }
}
